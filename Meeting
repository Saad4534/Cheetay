#include<isotream>
using namespace std;


int maxMeetings(int start[], int end[], int n)
{
	vector<pair<int, pair<int,="" int="">>>v;
	
	for (int i = 0; i < n; i++) {
		v.push_back({end[i], {start[i], i}});
	}
	
	sort(v.begin(), v.end());
	int count = 1;
	int curr = v[0].first;
	int i = 1;
	
	while (i < n) {
		if (curr < v[i].second.first)
		{
			count++;
			curr = v[i].first;
		}
		i++;
	}
	return count;
}


int main(){
	
	int n = 6;
	int s[] = {1,3,0,5,8,5};
	int f[] = {2,4,6,7,9,9};
	
	cout<<maxMeetings(s,f,n);
	
}
